Object subclass: #Driver	instanceVariableNames: 'driverID name rating assignedRides'	classVariableNames: ''	package: 'RideSystem'!!Driver methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:15'!initWith: id name: n rating: r    driverID := id.    name := n.    rating := r.    assignedRides := OrderedCollection new.! !!Driver methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:17'!showDriverInfo    ^ name , ' (ID: ', driverID asString, ', Rating: ', rating asString , ')'.! !!Driver methodsFor: 'adding' stamp: '11/16/2025 23:15'!addRide: aRide    assignedRides add: aRide.! !Object subclass: #TravelPlan	instanceVariableNames: 'tID origin destination miles cost'	classVariableNames: ''	package: 'RideSystem'!!TravelPlan methodsFor: 'as yet unclassified' stamp: '11/16/2025 22:53'!"this class handles trip info"initWith: id origin: o destination: d miles: m    tID := id.    origin := o.    destination := d.    miles := m.    cost := 0.! !!TravelPlan methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:01'!computeCost    self subclassResponsibility.! !!TravelPlan methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:03'!showPlan    ^ 'Ride ', tID asString, ' from ', origin, ' to ', destination,       ' (', miles asString, ' miles)'.! !TravelPlan subclass: #StandardRide	instanceVariableNames: ''	classVariableNames: ''	package: 'RideSystem'!!StandardRide methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:09'!computeCost    cost := miles * 1.7.    ^ cost.! !TravelPlan subclass: #PremiumRide	instanceVariableNames: ''	classVariableNames: ''	package: 'RideSystem'!!PremiumRide methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:11'!computeCost    cost := miles * 2.4.    ^ cost.! !Object subclass: #Rider	instanceVariableNames: 'riderID name requestedRides'	classVariableNames: ''	package: 'RideSystem'!!Rider methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:18'!initWith: id name: n    riderID := id.    name := n.    requestedRides := OrderedCollection new.! !!Rider methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:18'!requestRide: aRide    requestedRides add: aRide.! !!Rider methodsFor: 'as yet unclassified' stamp: '11/16/2025 23:19'!viewRides    ^ requestedRides collect: [ :r | r showPlan ].! !